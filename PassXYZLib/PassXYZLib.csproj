<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.2.5.0</Version>
    <AssemblyVersion>1.2.5.0</AssemblyVersion>
    <FileVersion>1.2.5.0</FileVersion>
    <Company>PassXYZ Inc.</Company>
    <Authors>Roger Ye</Authors>
    <Copyright>Roger Ye</Copyright>
    <PackageReleaseNotes>- Generate icons from URL field
	
- Using icons from FontAwesome

- Fixed an issue in CurrentPath</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/passxyz/KPCLib</PackageProjectUrl>
    <RepositoryUrl>https://github.com/passxyz/KPCLib</RepositoryUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.11.34" />
    <PackageReference Include="PassXYZ" Version="2.1.4.9" />
    <PackageReference Include="Xamarin.Essentials" Version="1.6.1" />
    <PackageReference Include="Xamarin.Forms" Version="5.0.0.2012" />
    <PackageReference Include="Xamarin.Forms.FontAwesome.Brand" Version="1.0.0" />
    <PackageReference Include="Xamarin.Forms.FontAwesome.Regular" Version="1.0.0" />
    <PackageReference Include="Xamarin.Forms.FontAwesome.Solid" Version="1.0.0" />
    <PackageReference Include="Xamarin.Forms.IconFontHelper" Version="1.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\KPCLib\KPCLib.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources\PxRes.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>PxRes.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources\PxRes.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>PxRes.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
